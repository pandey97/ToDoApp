{
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "@shared-constants": "./src/shared/constants",
                    "@shared-localization": "./src/shared/localization",
                    "@components-button": "./src//components/button",
                    "@components-FloatingLabelInput": "./src/components/FloatingLabelInput",
                    "@components-header": "./src/components/header",
                    "@core-ApiConstants": "./src/core/ApiConstants",
                    "@hocs-loderHOC": "./src/hocs/loderHOC",
                    "@assets": "./src/assets/Glyphs"
                },
                "extensions": [
                    ".js",
                    ".jsx",
                    ".ts",
                    ".tsx"
                ]
            }
        ]
    ]
}